{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://*.facebook.com/*", "https://*.workplace.com/*" ]
   } ],
   "description": "Share your screen in video calls on Workplace",
   "externally_connectable": {
      "matches": [ "https://*.facebook.com/*", "https://*.workplace.com/*" ]
   },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "34",
   "name": "Workplace Screen Sharing",
   "permissions": [ "desktopCapture", "notifications", "https://*.facebook.com/", "https://*.workplace.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}
